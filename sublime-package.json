{
  "contributions": {
    "settings": [
      {
        "schema": {
          "$id": "sublime://settings/LSP-aleo-developer",
          "definitions": {
            "PluginConfig": {
              "properties": {
                "settings": {
                  "additionalProperties": false,
                  "properties": {
                    "aleo-developer.validate": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable/disable validation feature"
                    },
                    "aleo-developer.completion": {
                      "type": "boolean",
                      "default": true,
                      "description": "Enable/disable completion feature"
                    },
                    "aleo-developer.maxNumberOfProblems": {
                      "scope": "resource",
                      "type": "number",
                      "default": 100,
                      "description": "Controls the maximum number of problems produced by the server."
                    },
                    "aleo-developer.trace.server": {
                      "scope": "window",
                      "type": "string",
                      "enum": ["off", "messages", "verbose"],
                      "default": "verbose",
                      "description": "Traces the communication between VS Code and the language server."
                    }
                  }
                }
              }
            }
          },
          "allOf": [
            {
              "$ref": "sublime://settings/LSP-plugin-base"
            },
            {
              "$ref": "sublime://settings/LSP-aleo-developer#/definitions/PluginConfig"
            }
          ]
        },
        "file_patterns": ["/LSP-aleo-developer.sublime-settings"]
      },
      {
        "schema": {
          "properties": {
            "settings": {
              "properties": {
                "LSP": {
                  "properties": {
                    "LSP-aleo-developer": {
                      "$ref": "sublime://settings/LSP-aleo-developer#/definitions/PluginConfig"
                    }
                  }
                }
              }
            }
          }
        },
        "file_patterns": ["/*.sublime-project"]
      }
    ]
  }
}
